# AWS values
export AWS_REGION=us-east-2
export EKS_CLUSTER_NAME=my-cluster

# external image repo (e.g. DockerHub) values
export REGISTRY=user/build-service # Assuming DockerHub account
export REG_USERNAME=username
export REG_PASSWORD=password

# TAP values
export TN_USERNAME=user@somewhere.com
export TN_PASSWORD=password
export DB_PASSWORD=persistent-password # Must be the same between deploys to the same cluster
